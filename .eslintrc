{
  "parser": "babel-eslint",
  "extends": ["plugin:import/errors", "plugin:import/warnings", "prettier"],
  "plugins": ["node", "prettier"],
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "prettier/prettier": 1,
    "node/exports-style": ["error", "module.exports"],
    "global-require": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "import/default": "off",
    "no-unused-vars": "error",
    "no-undefined": "error",
    "no-undef": "error",
    "quotes": ["error", "double"]
  }
}
